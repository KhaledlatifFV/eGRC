            <header class="flex items-center justify-between bg-white drop-shadow-md sticky top-0">
                <a class="text-white text-lg font-bold" asp-area="" asp-controller="Home" asp-action="Index"></a>
                <partial name="_LoginPartial" />
            </header>
            <section class="p-4">
